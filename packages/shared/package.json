{
  "name": "@movk-repo/shared",
  "type": "module",
  "version": "0.0.1",
  "packageManager": "pnpm@9.11.0",
  "description": "Shared utilities for movk",
  "author": "yixuan",
  "license": "MIT",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/node/index.js"
    },
    "./utils": {
      "types": "./dist/utils/index.d.ts",
      "default": "./dist/utils/index.js"
    },
    "./hooks": {
      "types": "./dist/hooks/index.d.ts",
      "default": "./dist/hooks/index.js"
    },
    "./constants": {
      "types": "./dist/constants/index.d.ts",
      "default": "./dist/constants/index.js"
    },
    "./types": {
      "types": "./dist/types/index.d.ts",
      "default": "./dist/types/index.js"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": "^18.0.0 || >=20.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "stub": "tsup --watch",
    "build": "tsup"
  },
  "peerDependencies": {
    "vue": "^3.2.0"
  },
  "dependencies": {
    "tinycolor2": "^1.6.0"
  },
  "devDependencies": {
    "@types/tinycolor2": "^1.4.6"
  }
}

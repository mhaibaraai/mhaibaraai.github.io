{
  "name": "@movk-repo/shared",
  "type": "module",
  "version": "1.0.0",
  "packageManager": "pnpm@9.7.1",
  "description": "Shared utilities for movk",
  "author": "yixuan",
  "license": "MIT",
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./utils": {
      "import": "./dist/utils.mjs",
      "require": "./dist/utils.cjs"
    },
    "./composables": {
      "import": "./dist/composables.mjs",
      "require": "./dist/composables.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "package.json"
  ],
  "scripts": {
    "stub": "tsup --watch",
    "build": "tsup",
    "clean": "rm -rf dist",
    "prebuild": "pnpm run clean"
  },
  "peerDependencies": {
    "vue": "^3.2.0"
  },
  "dependencies": {
    "tinycolor2": "^1.6.0"
  },
  "devDependencies": {
    "@types/tinycolor2": "^1.4.6"
  }
}

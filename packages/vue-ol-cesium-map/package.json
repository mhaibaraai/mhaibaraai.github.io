{
  "name": "vue-ol-cesium-map",
  "type": "module",
  "version": "1.0.0",
  "packageManager": "pnpm@9.1.0",
  "description": "这是一个用于OpenLayers和Cesium地图的Vue3组件",
  "author": "yixuan",
  "license": "MIT",
  "homepage": "https://github.com/mhaibaraai/ol-cesium-map",
  "repository": {
    "type": "git",
    "url": "git@github.com:mhaibaraai/ol-cesium-map.git"
  },
  "sideEffects": false,
  "main": "src/index.ts",
  "publishConfig": {
    "main": "dist/index.mjs",
    "module": "dist/index.mjs",
    "types": "dist/index.d.ts",
    "exports": {
      ".": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      }
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./*"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build"
  },
  "dependencies": {
    "ol": "^9.1.0"
  },
  "devDependencies": {
    "@types/lodash-es": "^4.17.12",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vunk/core": "^0.0.0-alpha.118",
    "lodash-es": "^4.17.21",
    "mitt": "^3.0.1",
    "vite": "^5.2.8",
    "vite-plugin-dts": "^3.9.0",
    "vue": "^3.4.21"
  }
}

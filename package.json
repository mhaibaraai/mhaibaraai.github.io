{
  "type": "module",
  "private": true,
  "packageManager": "pnpm@9.1.0",
  "engines": {
    "node": ">=18.20.0",
    "pnpm": ">=8.15.6"
  },
  "scripts": {
    "docs": "pnpm -C docs run dev",
    "docs:build": "pnpm -C docs run build",
    "lint": "turbo lint",
    "lint:packages": "turbo lint --filter='./packages/*'",
    "stub:auto-nav-sidebar": "turbo stub --filter=./packages/auto-nav-sidebar",
    "build": "turbo build",
    "build:packages": "turbo build --filter='./packages/*'",
    "build:auto-nav-sidebar": "turbo build --filter='./packages/auto-nav-sidebar'",
    "rm:nm": "find . -name 'node_modules' -type d -prune -execdir rm -rf '{}' +"
  },
  "devDependencies": {
    "@movk-repo/eslint-config": "workspace:*",
    "@movk-repo/typescript-config": "workspace:*",
    "@types/fs-extra": "^11.0.4",
    "@types/lodash-es": "^4.17.12",
    "bumpp": "^9.4.1",
    "fast-glob": "^3.3.2",
    "fs-extra": "^11.2.0",
    "lodash-es": "^4.17.21",
    "mri": "1.2.0",
    "tsup": "^8.0.2",
    "turbo": "^1.3.0",
    "unbuild": "^2.0.0",
    "vite": "^5.2.8",
    "vue": "^3.4.21"
  }
}

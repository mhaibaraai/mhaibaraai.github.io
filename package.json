{
  "name": "@movk-repo/core",
  "type": "module",
  "version": "1.1.0",
  "private": true,
  "packageManager": "pnpm@9.5.0",
  "engines": {
    "node": ">=18.20.0",
    "pnpm": ">=8.15.6"
  },
  "scripts": {
    "docs": "pnpm -C docs run dev",
    "docs:build": "pnpm -C docs run build",
    "turbo:lint": "turbo lint",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "stub:auto-nav-sidebar": "turbo stub --filter=./packages/auto-nav-sidebar",
    "build": "turbo build",
    "build:packages": "turbo build --filter='./packages/*'",
    "build:auto-nav-sidebar": "turbo build --filter='./packages/auto-nav-sidebar'",
    "release": "bumpp -r",
    "rm:nm": "find . -name 'node_modules' -type d -prune -execdir rm -rf '{}' +"
  },
  "dependencies": {
    "@arcgis/core": "^4.30.8",
    "@vuesri/core": "^1.0.41",
    "arcgis-js-api": "^4.30.8",
    "element-plus": "^2.7.7",
    "lodash-es": "^4.17.21",
    "three": "^0.164.1",
    "vue": "^3.4.31"
  },
  "devDependencies": {
    "@movk-repo/eslint-config": "workspace:*",
    "@movk-repo/shared": "workspace:*",
    "@movk-repo/typescript-config": "workspace:*",
    "@types/fs-extra": "^11.0.4",
    "@types/lodash-es": "^4.17.12",
    "@types/three": "^0.164.1",
    "bumpp": "^9.4.1",
    "fast-glob": "^3.3.2",
    "fs-extra": "^11.2.0",
    "sass": "^1.77.8",
    "tsup": "8.0.2",
    "turbo": "^2.0.6",
    "vite": "^5.3.3"
  }
}

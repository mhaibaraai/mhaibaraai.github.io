{
  "name": "@movk-repo/root",
  "type": "module",
  "version": "1.0.0",
  "private": true,
  "packageManager": "pnpm@9.7.1",
  "engines": {
    "node": ">=18.20.0",
    "pnpm": ">=8.15.6"
  },
  "scripts": {
    "docs": "pnpm -C docs run dev",
    "docs:build": "pnpm -C docs run build",
    "turbo:lint": "turbo lint",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "build:packages": "turbo build --filter='./packages/**'",
    "release": "bumpp -r",
    "rm:nm": "find . -name 'node_modules' -type d -prune -execdir rm -rf '{}' +"
  },
  "dependencies": {
    "@movk-repo/components": "workspace:*",
    "@movk-repo/constants": "workspace:*",
    "@movk-repo/eslint-config": "workspace:*",
    "@movk-repo/shared": "workspace:*",
    "@movk-repo/theme-chalk": "workspace:*",
    "@movk-repo/typescript-config": "workspace:*",
    "@movk-repo/utils": "workspace:*",
    "@vueuse/core": "^11.0.0",
    "element-plus": "^2.8.1",
    "lodash-es": "^4.17.21",
    "vue": "^3.4.38"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^18.19.25",
    "@types/three": "^0.164.1",
    "bumpp": "^9.4.1",
    "fast-glob": "^3.3.2",
    "fs-extra": "^11.2.0",
    "ts-node": "^10.9.2",
    "tsup": "^8.2.4",
    "turbo": "^2.0.6",
    "unplugin-auto-import": "^0.18.2",
    "unplugin-vue-components": "^0.27.4",
    "vite": "^5.4.1"
  }
}
